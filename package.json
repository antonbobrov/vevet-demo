{
  "name": "app",
  "version": "1.0.0",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint . --ext .jsx,.js,.tsx,.ts",
    "lint:style": "npx stylelint \"**/*.scss\"",
    "webpack:dev": "cross-env NODE_ENV=development webpack serve --config config/webpack/webpack.dev.js",
    "webpack:prod": "cross-env NODE_ENV=production webpack --config config/webpack/webpack.prod.js",
    "dev": "npm run webpack:dev",
    "build": "run-s lint webpack:prod",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": [
    "defaults",
    "not dead",
    "> 0.01%"
  ],
  "devDependencies": {
    "@anton.bobrov/eslint-config": "2.0.3",
    "@types/animejs": "^3.1.12",
    "autoprefixer": "^10.4.15",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "8.56.0",
    "gh-pages": "^6.1.1",
    "handlebars-loader": "^1.7.3",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^8.1.1",
    "prettier": "3.3.3",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.1",
    "stylelint": "^14.11.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.5.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "animejs": "^3.2.2",
    "normalize.css": "^8.0.1",
    "ts-essentials": "^10.0.2",
    "vevet": "^4.0.1"
  }
}
